<!-- begin page-header -->
<!-- end page-header -->
<!-- begin panel -->
<div class="panel panel-inverse">
  <div class="panel-heading">
    <h4 class="panel-title lp-header">Logs</h4>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-bordered data-table-no-buttons display nowrap">
			<thead>
				<tr class="odd gradeX">
					<th>Notifications</th>
				</tr>
			</thead>
			<tbody>
        <% Audited::Audit.order("created_at DESC").each do |audit| %>
          <tr>
            <td><%= audit.on_sentence.html_safe %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<!-- end panel -->
