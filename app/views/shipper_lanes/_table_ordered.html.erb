<style>
.table-bordered {
  border: 0px solid #dee2e6;
}
</style>
<table  class="table table-bordered table-responsive display nowrap">
  <thead>
    <tr>
      <th>Orig</th>
      <th>Dest</th>
      <th>Comm</th>
      <th>TPW</th>
      <th>Preferred Load Days</th>
      <th>Prefer Teams?</th>
    </tr>
  </thead>
  <tbody>
    <% if !lane_1.nil? %>
      <% lane_1.each do |shipper_lane| %>
        <tr class="lane-priority-<%= shipper_lane.lane_priority %>">
          <td>
            <% shipper_lane.lane_origin_to_array.each do |cr| %>
              <% if Shipper::LIGHT_BLUE.include?(cr) %>
                <span class="badge badge-primary"><%= cr %></span>
              <% elsif Shipper::DARK_BLUE.include?(cr) %>
                <span class="badge badge-drakblue"><%= cr %></span>
              <% elsif Shipper::BLUE.include?(cr) %>
                <span class="badge badge-info"><%= cr %></span>
              <% elsif Shipper::RED.include?(cr) %>
              <span class="badge badge-danger"><%= cr %></span>
              <% elsif Shipper::BROWN.include?(cr) %>
                <span class="badge badge-brown"><%= cr %></span>
              <% elsif Shipper::YELLOW.include?(cr) %>
                <span class="badge badge-yellow"><%= cr %></span>
              <% elsif Shipper::ORANGE.include?(cr) %>
                <span class="badge badge-warning"><%= cr %></span>
              <% elsif Shipper::GREEN.include?(cr) %>
                <span class="badge badge-green"><%= cr %></span>
              <% end %>
            <% end %>
          </td>
          <td>
            <% shipper_lane.lane_destination_to_array.each do |cr| %>
              <% if Shipper::LIGHT_BLUE.include?(cr) %>
                <span class="badge badge-primary"><%= cr %></span>
              <% elsif Shipper::DARK_BLUE.include?(cr) %>
                <span class="badge badge-drakblue"><%= cr %></span>
              <% elsif Shipper::BLUE.include?(cr) %>
                <span class="badge badge-info"><%= cr %></span>
              <% elsif Shipper::RED.include?(cr) %>
              <span class="badge badge-danger"><%= cr %></span>
              <% elsif Shipper::BROWN.include?(cr) %>
                <span class="badge badge-brown"><%= cr %></span>
              <% elsif Shipper::YELLOW.include?(cr) %>
                <span class="badge badge-yellow"><%= cr %></span>
              <% elsif Shipper::ORANGE.include?(cr) %>
                <span class="badge badge-warning"><%= cr %></span>
              <% elsif Shipper::GREEN.include?(cr) %>
                <span class="badge badge-green"><%= cr %></span>
              <% end %>
            <% end %>
          </td>
          <td>
            <% shipper_lane.commodities_to_array.each do |cr| %>
              <span class="badge badge-inverse"><%= cr %></span>
            <% end %>
          </td>
          <td><%= shipper_lane.truck_per_week %></td>
          <td><%= shipper_lane.preferred_load_day_initials %></td>
          <td><%= shipper_lane.prefer_team ? "<b class='text-success'>Yes</b>".html_safe : "<i class='text-danger'>No</i>".html_safe %></td>
        </tr>
      <% end %>
    <% end %>
