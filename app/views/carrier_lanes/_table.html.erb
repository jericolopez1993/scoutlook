<table  class="table table-striped table-bordered data-table-no-buttons display nowrap">
              <thead>
                <tr>
                  <th>Priority</th>
                  <th>Origin</th>
                  <th>Destination</th>
                  <th>Trucks Per Week</th>
                  <th>Preferred Load Days</th>
                  <th>Notes</th>
                  <th></th>
                </tr>
                </thead>
                <tbody>
                  <% if !carrier_lanes.nil? %>
                    <% carrier_lanes.each do |carrier_lane| %>
                      <tr>
                        <td><%= carrier_lane.lane_priority %></td>
                        <td><%= carrier_lane.lane_origin %></td>
                        <td><%= carrier_lane.lane_destination %></td>
                        <td><%= carrier_lane.truck_per_week %></td>
                        <td><%= carrier_lane.preferred_load_day %></td>
                        <td><%= carrier_lane.notes %></td>
                        <td>
                          <%= link_to '<i class="far fa-edit"></i>'.html_safe, edit_carrier_lane_path(carrier_lane), :class => 'btn btn-warning btn-xs' %>
                          <%= link_to '<i class="fas fa-trash-alt"></i>'.html_safe, carrier_lane, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-danger btn-xs'%>
                        </td>
                      </tr>
                    <% end %>
                  <% end %>
                </tbody>
              </table>
