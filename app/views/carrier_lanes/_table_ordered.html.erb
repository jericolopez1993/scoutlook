<table  class="table table-striped table-bordered data-table-unsorted display nowrap">
              <thead>
                <tr>
                  <th>Priority</th>
                  <th>Prefer Team</th>
                  <th>Origin</th>
                  <th>Destination</th>
                  <th>Trucks Per Week</th>
                  <th>Preferred Load Days</th>
                </tr>
                </thead>
                <tbody>
                  <% if !lane_1.nil? %>
                    <% lane_1.each do |carrier_lane| %>
                      <tr>
                        <td><%= carrier_lane.lane_priority_display %></td>
                        <td><%= carrier_lane.prefer_team ? "<b class='text-success'>Yes</b>".html_safe : "<i class='text-danger'>No</i>".html_safe %></td>
                        <td>
                          <% carrier_lane.lane_origin_to_array.each do |cr| %>
                            <span class="badge badge-success"><%= cr %></span>
                          <% end %>
                        </td>
                        <td>
                          <% carrier_lane.lane_destination_to_array.each do |cr| %>
                            <span class="badge badge-primary"><%= cr %></span>
                          <% end %>
                        </td>
                        <td><%= carrier_lane.truck_per_week %></td>
                        <td>
                          <% carrier_lane.preferred_load_day_to_array.each do |cr| %>
                            <span class="badge badge-inverse"><%= cr %></span>
                          <% end %>
                        </td>
                    </tr>
                    <% end %>
                  <% end %>
                  <% if !lane_2.nil? %>
                    <% lane_2.each do |carrier_lane| %>
                      <tr>
                        <td><%= carrier_lane.lane_priority_display %></td>
                        <td><%= carrier_lane.prefer_team ? "<b class='text-success'>Yes</b>".html_safe : "<i class='text-danger'>No</i>".html_safe %></td>
                        <td>
                          <% carrier_lane.lane_origin_to_array.each do |cr| %>
                            <span class="badge badge-success"><%= cr %></span>
                          <% end %>
                        </td>
                        <td>
                          <% carrier_lane.lane_destination_to_array.each do |cr| %>
                            <span class="badge badge-primary"><%= cr %></span>
                          <% end %>
                        </td>
                        <td><%= carrier_lane.truck_per_week %></td>
                        <td>
                          <% carrier_lane.preferred_load_day_to_array.each do |cr| %>
                            <span class="badge badge-inverse"><%= cr %></span>
                          <% end %>
                        </td>
                    </tr>
                    <% end %>
                  <% end %>
                  <% if !lane_3.nil? %>
                    <% lane_3.each do |carrier_lane| %>
                      <tr>
                        <td><%= carrier_lane.lane_priority_display %></td>
                        <td><%= carrier_lane.prefer_team ? "<b class='text-success'>Yes</b>".html_safe : "<i class='text-danger'>No</i>".html_safe %></td>
                        <td>
                          <% carrier_lane.lane_origin_to_array.each do |cr| %>
                            <span class="badge badge-success"><%= cr %></span>
                          <% end %>
                        </td>
                        <td>
                          <% carrier_lane.lane_destination_to_array.each do |cr| %>
                            <span class="badge badge-primary"><%= cr %></span>
                          <% end %>
                        </td>
                        <td><%= carrier_lane.truck_per_week %></td>
                        <td>
                          <% carrier_lane.preferred_load_day_to_array.each do |cr| %>
                            <span class="badge badge-inverse"><%= cr %></span>
                          <% end %>
                        </td>
                    </tr>
                    <% end %>
                  <% end %>
                </tbody>
              </table>
